<!DOCTYPE html>
<html lang="en">

  <head>
    <!-- Set meta values -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8">
    <!-- Suppress browser request for favicon.ico -->
    <link rel="shortcut icon"type="image/x-icon" href="data:image/x-icon;,">
    <!-- Title block -->
    <title>Andrew's Resume</title>
    <!-- Style Injection Area -->
    <style>
      @@include(@@coreNS)
      @@include(@@coreSS)
      @@include(@@pageSS)
      .envelope{
        background: url('data:image/svg+xml;utf8,@@include("../extras/envelope.svg")');
      }
      .linkedin{
        background: url('data:image/svg+xml;utf8,@@include("../extras/linkedin.svg")');
      }
      .github{
        background: url('data:image/svg+xml;utf8,@@include("../extras/github.svg")');
      }
    </style>
  </head>

  <body>
    <div style="height:1em;"></div>
    <div class="container">

      <!-- START OF INTRO -->
      <div class="row" id="personalInfo">
        <div class="one column">
          <div class="blob bg-green"></div>
        </div>

        <!-- **INJECTION AREA** -->

      </div>
      <!-- END OF INTRO -->

      <hr class="slim-bm slim-tm">

      <div class="row">

        <!-- START OF EDU/WORK EXP -->
        <div class="columns seven">

          <h5 class="b no-bm" id="experienceInfoSibling">WORK EXPERIENCE</h5>
          
          <!-- **INJECTION AREA** -->

          <h5 class="b no-bm">EDUCATION</h5>

          <div class="row" id="educationInfo">
            <div class="one columns">
              <div class="blob bg-gray"></div>
            </div>

            <!-- **INJECTION AREA** -->

          </div>

        </div>
        <!-- END OF EDU/WORK EXP -->

        <!-- START OF SKILLS -->
        <div class="columns five" id="skillsInfo">

          <!-- **INJECTION AREA** -->
          
        </div>
        <!-- END OF SKILLS -->

      </div>

    </div> <!-- CONTAINER -->


    <!-- IMPORT MICRO JS FRAMEWORK -->
    <script>
      @@include(@@coreJS)
    </script>

  	<!-- IMPORT PAGE JS -->
    <script>
      @@include(@@pageJS)
    </script>

  </body>
</html>
